<?xml version="1.0" encoding="utf-8" ?> 
<toast launch="tomighty">
  <visual>
    <binding template="ToastGeneric">
      <text>{interval_type} completed</text>
      <image src='{image_src}' placement='appLogoOverride'/>
    </binding>
  </visual>
  <actions>
    <action content="{action_content}" arguments="{action_args}" />
    <action content="Dismiss" arguments="dismiss" />
  </actions>
  <audio src="ms-winsoundevent:Notification.Reminder"/>
</toast>
